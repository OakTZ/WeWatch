{
    "name": "WeWatch",
    "version": "1.0",
    "manifest_version": 3,
    "icons":{
      "16":"pics/LOGO.png",
      "32":"pics/LOGO.png",
      "48":"pics/LOGO.png",
      "128":"pics/LOGO.png"
    },
    "permissions": [ 
      "tabs",
      "scripting",
      "activeTab",
      "webNavigation",
      "declarativeNetRequest",
      "storage"

    ],
    "host_permissions": [
      "*://www.youtube.com/*"

    ],
    "background": {
      "service_worker": "background.js"
      
      
    },
    "action":{
      "default_icon":{
        "16":"pics/LOGO.png",
        "32":"pics/LOGO.png",
        "48":"pics/LOGO.png",
        "128":"pics/LOGO.png"  
      },
      "default_popup": "htmls/dif_popup.html",
      "default_title": "WeWatch$"
    }
  
  }
  